{
  "name": "nimbus-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Nimbus UI Monorepo",
  "author": "moja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nimbus-ui-org/nimbus-ui.git"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build --filter=!docs",
    "lint": "eslint -c .eslintrc.cjs --ext .js,.ts,.jsx,.tsx . --max-warnings=0",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "clean": "turbo clean && rm -rf node_modules && rm -rf .turbo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "panda:pkg": "pnpm --filter @nimbus-ui/core prepare"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.45.1",
    "@pandacss/eslint-plugin": "^0.1.9",
    "@react-types/shared": "^3.24.1",
    "@types/eslint": "^8.56.11",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.2.4",
    "turbo": "^2.1.1",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  },
  "packageManager": "pnpm@9.9.0"
}
