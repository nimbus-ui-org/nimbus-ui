{
  "name": "nimbus-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Nimbus UI Monorepo",
  "author": "moja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nimbus-ui-org/nimbus-ui.git"
  },
  "scripts": {
    "storybook": "turbo dev dev:storybook",
    "dev:docs": "turbo dev dev:docs",
    "dev:storybook": "storybook dev -p 6006",
    "dev": "turbo dev",
    "build": "turbo build --filter=!docs",
    "build:preset": "turbo build --filter=@nimbus-ui/preset",
    "build:storybook": "storybook build",
    "lint": "eslint -c .eslintrc.cjs --ext .js,.ts,.jsx,.tsx . --max-warnings=0",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "clean": "turbo clean && rm -rf node_modules && rm -rf .turbo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "panda:pkg": "pnpm --filter @nimbus-ui/core prepare",
    "pack:local": "turbo pack:local",
    "pack:clean": "turbo pack:clean"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.46.0",
    "@pandacss/eslint-plugin": "^0.1.11",
    "@pandacss/preset-panda": "^0.46.0",
    "@react-types/shared": "^3.24.1",
    "@storybook/addon-a11y": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/addon-interactions": "^8.3.2",
    "@storybook/addon-links": "^8.3.2",
    "@storybook/blocks": "^8.3.2",
    "@storybook/react": "^8.3.2",
    "@storybook/react-vite": "^8.3.2",
    "@storybook/test": "^8.3.2",
    "@storybook/theming": "^8.3.2",
    "@types/eslint": "^8.56.12",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.3.2",
    "storybook-addon-data-theme-switcher": "^0.4.2",
    "storybook-addon-rtl": "^1.0.1",
    "storybook-dark-mode": "^4.0.2",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.0",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  },
  "packageManager": "pnpm@9.10.0"
}
